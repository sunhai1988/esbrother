<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.esbrother.system.entity.role.LpsfRole" table="lpsf_role">
<id column="id" name="id" type="string">
<generator class="uuid.hex"/>
</id>
<property column="name" length="120" name="name" type="string"/>
<property column="role_declare" length="200" name="roleDeclare" type="string"/>
<property column="role_isForbitten" length="10" name="roleIsforbitten" type="string"/>
<property column="remark" length="500" name="remark" type="string"/>
</class>
</hibernate-mapping>
